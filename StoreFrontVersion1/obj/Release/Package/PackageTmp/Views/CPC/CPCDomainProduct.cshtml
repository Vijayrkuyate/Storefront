@model IEnumerable<ModelView.CustomerProduct>
@{
    ViewBag.Title = "CPCDomainProduct";
    Layout = "~/Views/Shared/_LayoutCPC.cshtml";
}

<section class="content-header">
    <h1>
        @*Channal Partner Customer*@
        @*<small>Control panel</small>*@
    </h1>
    <ol class="breadcrumb">
       
        <li class="active"> Domain</li>
    </ol>
</section>
<section class="content">


    <div class="col-lg-12">
        <section class="col-lg-12 connectedSortable ui-sortable">
            <!-- Box (with bar chart) -->
            <div class="box box-danger" id="loading-example">
                <div class="box-header" style="cursor: move;background-color: #030358;cursor: move;color: white;">
                    <!-- tools box -->
                    <div class="form-group col-md-3" style="margin-top: 10px;">

                        <ol class="breadcrumb" style="background-color: #030358;">

                            <li class="active"><button class="btn btn-success btn-md" onclick="DownloadExcel()"><i class="fa fa-file-o"></i> ExportExcel</button></li>
                        </ol>

                    </div>
                  
                </div><!-- /.box-header -->
                <div class="box-body no-padding">
                    <div class="box-body table-responsive" id="OrderDtl">
                        <table id="example1" class="table table-bordered table-striped">
                            <thead>
                                <tr style="color: white;background-color: #030358;">

                                    <th align="center">Sr.No</th>
                                   
                                    <th>Order Date</th>
                                    <th>Product Name</th>
                                    <th>Prise</th>
                                    <th>Creation Date</th>
                                    <th>Expiration Date</th>
                                    <th>Deletion Date</th>
                                    <th>Operation</th>

                                </tr>
                            </thead>
                            <tbody>
                                @{ int rowNo = 0; }
                                @foreach (var item in Model)
                                {
                                    <tr>
                                        <td>
                                            @(rowNo += 1)
                                        </td>
                                        
                                        <td>@item.OrderDate</td>
                                        <td>@item.ProductName</td>
                                        <td>@item.ProductPrise</td>
                                        <td>@item.DomainCreationDate</td>
                                        <td>@item.DomainExpirationDate</td>
                                        <td>@item.DomainDeletionDate</td>
                                        <td align="center">
                                            <a class="btn btn-danger btn-sm" href="@Url.Action("EditDomain","CPC",new { CustDomainProductId=@item.CustDomainProductId})">
                                                <i class="fa fa-eye">
                                                </i>

                                            </a>
                                        </td>
                                    </tr>

                                }

                            </tbody>
                            @*<tfoot>
                                    <tr>
                                        <th align="center">Sr.No</th>
                                        <th>User Name</th>
                                        <th>Email Id</th>
                                        <th>Mobile No</th>
                                        <th>Address</th>
                                        <th>Operation</th>
                                    </tr>
                                </tfoot>*@
                        </table>
                    </div>
                </div><!-- /.box-body -->

            </div><!-- /.box -->
            <!-- Custom tabs (Charts with tabs)-->
            <!-- /.nav-tabs-custom -->
            <!-- Calendar -->
            <!-- /.box -->
            <!-- quick email widget -->


        </section>




    </div>


</section>
<script src="https://cdnjs.cloudflare.com/ajax/libs/jquery/1.11.2/jquery.min.js"></script>
<script src="~/Scripts/Excel/jquery.table2excel.js"></script>

<script type="text/javascript">
        $(function () {
        $("#example1").dataTable();
        $('#example2').dataTable({
            "bPaginate": true,
            "bLengthChange": false,
            "bFilter": false,
            "bSort": true,
            "bInfo": true,
            "bAutoWidth": false
        });
        });
        function write_to_excel() {
            $("#example1").table2excel({
                name: "Table2Excel",
                filename: "myFileName",
                fileext: ".xls"
            });
        }

</script>

