@model IEnumerable<ModelView.ProductEmail>

<table id="example1" class="table table-bordered table-striped">
    <thead>
        <tr style="color: white;background-color: #1c7faf;">

            <th style="width: 20px;">Image</th>
            <th>Product Code</th>
            <th>Product Name</th>
            <th>Cost Price</th>
            <th>Selling Price</th>
            <th>Creation Price</th>
            <th>Renewal Price</th>



        </tr>
    </thead>
    <tbody>
        @{ int rowNo = 0; }
        @foreach (var item in Model)
        {
            <tr>
                @*<td>@item.RegistrationDate</td>*@
                <td>
                    <div class="pull-left image">
                        <img src="~/Documents/ProductImage/@item.EmailImage" class="img-circle" alt="User Image" style="height: 47px;">
                    </div>
                </td>
                <td>@item.ProductCode</td>

                <td>@item.ProductName</td>
                <td>@item.CostPrice</td>
                <td>@item.SellingPrice</td>

                <td>@item.RegistrationPrice</td>
                <td>@item.RenewalPrice</td>


            </tr>
        }





    </tbody>
    <tfoot>
        <tr></tr>
    </tfoot>
</table>
